<%= erb :'banner_standard', :layout => false %>

<div class="product_container">
	<% @theme.each do |theme| %>
		<div id="<%= theme.name.downcase.gsub(" ", "_") %>" class="product">
			<div class="edit_theme"><a class="delete" href="/admin/delete/<%= theme.id %>">delete</a> <a class="edit" href="/admin/edit/<%= theme.id %>">edit</a></div>
			<div class="product_name"><%= theme.name %></div>
		</div>
	<% end %>
</div>